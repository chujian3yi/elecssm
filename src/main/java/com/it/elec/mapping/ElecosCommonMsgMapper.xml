<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper  namespace="com.it.elec.dao.ElecosCommonMsgDao">

	<resultMap type="ElecCommonMsg" id="baseResultMap">
		<id column="com_id" property="comId"/>
		<result column="station_run" property="stationRun"/>
		<result column="dev_run" property="devRun"/>
		<result column="create_time" property="createTime"/>
		<result column="update_time" property="updateTime"/>
		<result column="createemp_id" property="createempId"/>
		
	</resultMap>
 	<sql id="baseColumList">
 		com_id,station_run,dev_run,create_time,update_time,createemp_id
 	</sql>
 	
	<!-- 查询监控信息 -->
	<select id="listCommonMsg" resultMap="baseResultMap">
	select <include refid="baseColumList"/>
	from elecos_commonmsg 
	</select>
	
	<!-- save -->
	<insert id="save" parameterType="com.it.elec.model.ElecCommonMsg">
    insert into elecos_commonmsg
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="comId != null">
        com_id,
      </if>
      <if test="stationRun != null">
        station_run,
      </if>
      <if test="devRun != null">
        dev_run,
      </if>
      <if test="createTime != null">
        create_time,
      </if>
      <if test="updateTime != null">
        update_time,
      </if>
      <if test="createempId != null">
        createemp_id,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="comId != null">
        #{comId,jdbcType=INTEGER},
      </if>
      <if test="stationRun != null">
        #{stationRun,jdbcType=VARCHAR},
      </if>
      <if test="devRun != null">
        #{devRun,jdbcType=VARCHAR},
      </if>
      <if test="createTime != null">
        #{createTime,jdbcType=TIMESTAMP},
      </if>
      <if test="updateTime != null">
        #{updateTime,jdbcType=TIMESTAMP},
      </if>
      <if test="createempId != null">
        #{createempId,jdbcType=INTEGER},
      </if>
    </trim>
  </insert>
	
</mapper>